{
  "presets": [
    "react",
    "es2015",
    "env"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true 
      }
    ],
    [
      "transform-react-jsx",
      {
        "pragma": "React.createElement"
      }
    ]
  ],
  "parserOpts": {}
}
